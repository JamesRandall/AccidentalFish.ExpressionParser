<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/JamesRandall/AccidentalFish.ExpressionParser</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/JamesRandall/AccidentalFish.ExpressionParser</PackageProjectUrl>
    <RepositoryUrl>https://github.com/JamesRandall/AccidentalFish.ExpressionParser</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <Version>0.11.0</Version>
    <Description>Converts an expression tree generated by AccidentalFish.ExpressionParser into a LINQ expression</Description>
    <Copyright>Copyright Â© James Randall 2017</Copyright>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


  <ItemGroup>
    <ProjectReference Include="..\AccidentalFish.ExpressionParser\AccidentalFish.ExpressionParser.csproj" />
  </ItemGroup>

</Project>
